<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
<servlet-name>FileSaveServlet</servlet-name>
<servlet-class>com.akjava.mbl3d.expression.server.FileSaveServlet</servlet-class>

<init-param>
<param-name>OutputDir</param-name>
<param-value>e:\images\</param-value>
    </init-param>
    
</servlet>
<servlet-mapping>
<servlet-name>FileSaveServlet</servlet-name>
<url-pattern>/write</url-pattern>
</servlet-mapping>
  
  <!-- Default page to serve -->

</web-app>
